UPDATE Projects
SET budget = 20000
WHERE budget IS NULL;

UPDATE Projects
SET status = 'Ongoing'
WHERE status IS NULL;

DELETE FROM Projects
WHERE client_name IS NULL;
select * from projects;

UPDATE Projects
SET end_date = '2025-12-31'
WHERE end_date IS NULL;

