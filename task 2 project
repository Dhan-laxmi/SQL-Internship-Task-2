CREATE TABLE Projects (
    project_id INTEGER PRIMARY KEY,
    project_name text NOT NULL,
    client_name TEXT,
    start_date DATE,
    end_date DATE,
    budget REAL DEFAULT 0.0,
    status TEXT DEFAULT 'Ongoing'
);
